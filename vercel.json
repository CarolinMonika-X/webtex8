{
  "version": 2,
  "builds": [
    {
      "src": "server-app/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/donors(.*)",
      "dest": "server-app/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/index.html"
    }
  ]
}
